\n              className=\"close_modal\"\r\n              style={{\r\n                fontSize: \"3rem\",\r\n                color: \"white\",\r\n                backgroundColor: \"red\",\r\n                borderRadius: \"50%\",\r\n              }}\r\n              onClick={() => {\r\n                setModal(false);\r\n              }}\r\n            />\r\n            <img src={IMG_modal} alt=\"\" className=\"img\" />\r\n          </div>\r\n        </div>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\nexport default AddImages;\r\n","C:\\full stack\\apartments\\client\\src\\components\\Add_&_Edit_Apartment\\Description\\Description.js",["358"],"C:\\full stack\\apartments\\client\\src\\components\\ListApartment&delete&edit\\editApartment.js",["359","360"],"import React, { useState, useContext } from \"react\";\r\nimport AddEditApartment from \"../Add_&_Edit_Apartment/Add_&_Edit_Apartment\";\r\n\r\nconst EditApartment = ({ itemForEdit, setIsOpenForEdit }) => {\r\n  return (\r\n    <div>\r\n      <AddEditApartment\r\n        itemForEdit={itemForEdit}\r\n        setIsOpenForEdit={setIsOpenForEdit}\r\n      />\r\n    </div>\r\n  );\r\n};\r\nexport default EditApartment;\r\n","C:\\full stack\\apartments\\client\\src\\components\\Input_select_button\\Input_select_button.js",["361"],"import \"./Input_select_button.css\";\r\nimport ExpandMoreIcon from \"@mui/icons-material/ExpandMore\";\r\n\r\nexport const Input = (props) => {\r\n  const {\r\n    label,\r\n    placeholder,\r\n    type,\r\n    name,\r\n    onChange,\r\n    formikErr,\r\n    width,\r\n    content,\r\n    value,\r\n    defaultValue,\r\n  } = props;\r\n\r\n  return (\r\n    <div className=\"div_input\" style={{ width: width }}>\r\n      <label className=\"label_input\">{label}</label>\r\n      <input\r\n        className=\"input\"\r\n        placeholder={placeholder}\r\n        type={type ? type : \"text\"}\r\n        name={name}\r\n        onChange={onChange}\r\n        value={value}\r\n        defaultValue={defau